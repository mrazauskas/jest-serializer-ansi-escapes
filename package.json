{
  "name": "jest-serializer-ansi-escapes",
  "version": "2.0.1",
  "description": "Jest snapshot serializer for ANSI escape sequences",
  "keywords": [
    "ansi",
    "escape",
    "sequences",
    "jest",
    "snapshots",
    "serializer",
    "testing",
    "cursor",
    "colors",
    "cli",
    "console",
    "terminal"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrazauskas/jest-serializer-ansi-escapes.git"
  },
  "license": "MIT",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src/*.js",
    "src/*.d.ts"
  ],
  "scripts": {
