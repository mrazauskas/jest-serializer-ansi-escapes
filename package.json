    "babel-jest": "29.7.0",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "27.6.3",
    "jest": "29.7.0",
    "prettier": "3.2.4",
    "pretty-format": "29.7.0"
  },
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": ">=16"
